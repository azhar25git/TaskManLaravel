{
    "name": "azhar25git/taskman",
    "description": "Task Manager Package",
    "license": "MIT",
    "authors": [
        {
            "name": "Azhar Uddin",
            "email": "getazharuddin@gmail.com",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/azhar25git/taskman",
    "keywords": ["Laravel", "TaskMan"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Azhar25git\\TaskMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Azhar25git\\TaskMan\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Azhar25git\\TaskMan\\TaskManServiceProvider"
            ],
            "aliases": {
                "TaskMan": "Azhar25git\\TaskMan\\Facades\\TaskMan"
            }
        }
    }
}
